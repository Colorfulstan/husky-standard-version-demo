        "protocol_override_domains" : {"googlesyndication" : "http", "www.google-analytics.com" : "https", "googletagservices" : "http", "scorecardresearch" : "http", "jsdelivr" : "http", "cloudflare" : "http", "127.0.0.1" : "https", "localhost" : "https" },
        "externally_connectable":{
            "matches":[ "http://api.tasty.gg", "http://cdnjs.cloudflare.com", "http://www.googletagservices.com", "http://b.scorecardresearch.com", "http://cdn.jsdelivr.net", "https://127.0.0.1", "https://localhost" ]
