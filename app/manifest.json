{
    "manifest_version":1,
    "type":"WebApp",
    "meta":{
        "name":"LoLwiz",
        "version":"4.0.2.1",
        "minimum-overwolf-version": "0.110.1",
        "author":"MobaBeast",
        "icon":"IconMouseOver.png",
        "icon_gray":"IconMouseNormal.png",
        "launcher_icon": "LoLwiz.ico",
        "description":"Dominate your opponents with LoLwiz"
